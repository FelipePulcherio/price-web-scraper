{
  "name": "backend",
  "version": "0.0.1",
  "description": "Scrape pre defined websites in search for a product price and store data in MongoDB",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
