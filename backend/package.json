    "Scraper",
    "Price"
  ],
  "author": "Felipe S. Pulcherio",
  "license": "ISC",
  "dependencies": {
    "@hokify/agenda": "^6.3.0",
    "@prisma/client": "^6.3.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mongoose": "^8.5.2",
    "puppeteer-core": "^23.6.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.2",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "prisma": "^6.2.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
